server {
        server_name localhost;

        listen  80;
        listen  [::]:80 default ipv6only=on;

        root /var/www/bottlebase;

        access_log /var/log/nginx/lurkdis.access.log;
        error_log /var/log/nginx/lurkdis.error.log;

        location / {
                uwsgi_pass unix:/tmp/uwsgi.lurkdis.socket;
                include uwsgi_params;
        }
}
